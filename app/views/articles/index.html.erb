<h1>this is the index </h1>

<% @articles.each do |article| %>
  <h2><%= article.title %></h2>
  <p>
    Published at
    <%= article.created_at.strftime('%b, %d, %y') %>
  </p>
  <p>
    <%= truncate(article.content, length: 200) %>
  </p>
<% end %>



<%= link_to "New Article", new_article_path %>
